<Configuration status="WARN">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%C %F %L %M %d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="INFO">
            <AppenderRef ref="Console"/>
        </Root>
            <Logger name="com.company.demoapplication" level="DEBUG"/>

<!--        <Logger name="org.apache.http.impl.execchain.MainClientExec" level="DEBUG"/>-->
<!--        <Logger name="org.apache.http.wire" level="DEBUG"/>-->
<!--        <Logger name="com.amazonaws.request" level="DEBUG"/>-->
<!--        <Logger name="com.amazonaws.http" level="DEBUG"/>-->
    </Loggers>
</Configuration>
